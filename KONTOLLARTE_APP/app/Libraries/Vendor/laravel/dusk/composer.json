{
    "name": "laravel/dusk",
    "description": "Laravel Dusk provides simple end-to-end testing and browser automation.",
    "keywords": ["laravel", "testing", "webdriver"],
    "license": "MIT",
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "facebook/webdriver": "^1.3",
        "nesbot/carbon": "^1.20|^2.0",
        "illuminate/console": "~5.7.0|~5.8.0",
        "illuminate/support": "~5.7.0|~5.8.0",
        "symfony/console": "^4.0",
        "symfony/finder": "^4.0",
        "symfony/process": "^4.0",
        "vlucas/phpdotenv": "^2.2|^3.3"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Laravel\\Dusk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravel\\Dusk\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.0-dev"
        },
        "laravel": {
            "providers": [
                "Laravel\\Dusk\\DuskServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
